{
    "name": "orchestra/canvas",
    "description": "Code Generators for Laravel Applications and Packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "bin": [
        "canvas"
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Canvas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Canvas\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2 || >=8.0",
        "illuminate/database": "^6.15",
        "illuminate/support": "^6.15",
        "orchestra/canvas-core": "^4.8",
        "symfony/yaml": "^4.3 || ^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^4.10",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Orchestra\\Canvas\\LaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
